                        </Button>
                        <Button x:Name="btSetting3"    Height="60" Width="215" >

                            <TextBlock  Text="Input CH1" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting4" Background="#333333"  Height="60" Width="215" >

