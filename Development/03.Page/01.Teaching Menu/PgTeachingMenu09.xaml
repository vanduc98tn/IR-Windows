<Page x:Class="Development.PgTeachingMenu09"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Development"
      mc:Ignorable="d" 
      d:DesignHeight="790" d:DesignWidth="1300"
      Title="PgTeachingMenu09">

    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1">
            <Grid.Resources>

                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="24" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource sButton}">
                    <Setter Property="FontSize" Value="24" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Margin" Value="0,1,5,1" />
                    <Setter Property="Background" Value="White" />
                    <Setter Property="BorderThickness" Value="0"/>
                    <Setter Property="HorizontalContentAlignment" Value="Left"/>

                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="20" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>

                </Style>

            </Grid.Resources>
            <Border>
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="#6787c0" Offset="0"/>
                        <GradientStop Color="White" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <Grid>
                    <Canvas>
                        <Polygon Points="0,20,130,80,0,170" Fill="#879ec8"/>
                        <Polygon Points="100,400,200,370,180,470" Fill="#879ec8"/>
                        <Ellipse Margin="250 450 0 0" Width="40" Height="40" Fill="#879ec8"/>
                        <Ellipse Margin="50 400 0 0" Width="40" Height="40" Fill="#879ec8"/>

                    </Canvas>
                    <StackPanel VerticalAlignment="Bottom">
                        <TextBlock Foreground="#879ec8" Text="ITM" Style="{StaticResource titleText}"/>
                        <TextBlock Foreground="#879ec8" Text="Semiconductor" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0" FontWeight="Bold"/>
                        <TextBlock Foreground="#879ec8" Text="AUTOMATION TEAM" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" >
                        <Label  FontWeight="Bold" Padding="0 20 0 0" Foreground="White" FontSize="27" HorizontalAlignment="Center">
                            <Label.Effect>
                                <DropShadowEffect Color="Black" Direction="320" ShadowDepth="5" Opacity="0.3"/>
                            </Label.Effect> System Menu
                        </Label>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="0 100 0 0" >
                        <Button x:Name="btSetting1"  Height="60" Width="215"  Margin="0 5 5 5" >

                            <TextBlock Text="Manual" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>


                        <Button x:Name="btSetting2" Height="60" Width="215" >

                            <TextBlock  Text="Load Tray"   Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting3"   Height="60" Width="215" >

                            <TextBlock  Text="Input CH1" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting4"  Height="60" Width="215" >

                            <TextBlock  Text="Input CH2"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting5"  Height="60" Width="215" >

                            <TextBlock  Text="Tester Rotary"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting6"  Height="60" Width="215" >

                            <TextBlock  Text="Output CH1"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting7"  Height="60" Width="215" >

                            <TextBlock  Text="Output CH2"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting8"  Height="60" Width="215" >

                            <TextBlock  Text="Tapping"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting9"  Height="60" Width="215" >

                            <TextBlock  Text="Data Matrix"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting10" Background="#333333" Height="60" Width="215" >

                            <TextBlock  Text="Offset Data" Foreground="White" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>





                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
        <Grid Grid.Column="0" Margin="0 0 2 0">

