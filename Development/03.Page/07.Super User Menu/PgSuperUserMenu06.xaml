    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="1">
            <Grid.Resources>

                <Style TargetType="{x:Type Label}">
                    <Setter Property="Foreground" Value="#dd222222" />
                    <Setter Property="Padding" Value="5,0" />
                    <Setter Property="VerticalContentAlignment" Value="Center" />
                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                    <Setter Property="FontSize" Value="24" />
                </Style>
                <Style TargetType="Button" BasedOn="{StaticResource sButton}">
                    <Setter Property="FontSize" Value="24" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Margin" Value="0,5,5,5" />
                    <Setter Property="Background" Value="White" />
                    <Setter Property="BorderThickness" Value="0"/>

                </Style>
                <Style TargetType="{x:Type TextBlock}">
                    <Setter Property="VerticalAlignment" Value="Center"/>
                    <Setter Property="FontSize" Value="20" />
                    <Setter Property="FontWeight" Value="Bold" />
                    <Setter Property="Foreground" Value="Black"/>

                </Style>

            </Grid.Resources>
            <Border>
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                        <GradientStop Color="#6787c0" Offset="0"/>
                        <GradientStop Color="White" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <Grid>
                    <Canvas>
                        <Polygon Points="0,20,130,80,0,170" Fill="#879ec8"/>
                        <Polygon Points="100,400,200,370,180,470" Fill="#879ec8"/>
                        <Ellipse Margin="250 450 0 0" Width="40" Height="40" Fill="#879ec8"/>
                        <Ellipse Margin="50 400 0 0" Width="40" Height="40" Fill="#879ec8"/>

                    </Canvas>
                    <StackPanel VerticalAlignment="Bottom">
                        <TextBlock Foreground="#879ec8" Text="ITM" Style="{StaticResource titleText}"/>
                        <TextBlock Foreground="#879ec8" Text="Semiconductor" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0" FontWeight="Bold"/>
                        <TextBlock Foreground="#879ec8" Text="AUTOMATION TEAM" Style="{StaticResource normalText}" Opacity="0.8" Margin=" 20 0"/>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" >
                        <Label  FontWeight="Bold" Padding="0 20 0 0" Foreground="White" FontSize="27" HorizontalAlignment="Center">
                            <Label.Effect>
                                <DropShadowEffect Color="Black" Direction="320" ShadowDepth="5" Opacity="0.3"/>
                            </Label.Effect> System Menu
                        </Label>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="0 100 0 0" >



                        <Button x:Name="btSetting1"  Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Delay Cylinder"  Margin="5"/>

                        </Button>


                        <Button x:Name="btSetting2"  Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0">

                            <TextBlock  Text="Delay Work"  Margin="5"/>

                        </Button>

                        <Button x:Name="btSetting3"  Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0">

                            <TextBlock  Text="Servo Prm Tray"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>

                        <Button x:Name="btSetting4"  Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0">

                            <TextBlock  Text="Servo Prm Input1"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting5" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Servo Prm Input2"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting6" Background="#333333" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Servo Prm Rotary" Foreground="White" Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting7" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Servo Prm Output1"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting8" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Servo Prm Output2"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>
                        <Button x:Name="btSetting9" Height="60" Width="215" Style="{StaticResource ssButton}" Margin="0" >

                            <TextBlock Text="Servo Prm Taping"  Margin="10" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                        </Button>



                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
